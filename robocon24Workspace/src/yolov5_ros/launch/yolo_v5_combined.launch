<?xml version="1.0" encoding="utf-8"?>

<launch>

  <!-- Load Parameter -->
  
  <param name="yolov5_path"       value="$(find yolov5_ros)/yolov5"/>
  <param name="use_cpu"           value="false" />

  <!-- Start yolov5 and ros wrapper -->
  <node pkg="yolov5_ros" type="yolo_v5_combined.py" name="yolo_v5_combined" output="screen" >
    <param name="weight_path"       value="$(find yolov5_ros)/weights/ball.engine"/>
    <param name="image_topic"       value="/camera/color/image_raw" />
    <param name="image_topic_2"       value="/camera_335/color/image_raw" />
    <param name="pub_topic"         value="/yolov5/BoundingBoxes" />
    <param name="camera_frame"       value="camera_color_frame"/>
    <param name="conf"              value="0.5" />
  </node>

</launch>
